</tr>
<tr>
	<td>include</td>
	<td>partials/expand_fieldset</td>
	<td>toggle_link_id=${tab_1}_${index_1}__voice_sections__${index_2}_show_link</td>
</tr>

<!--Set our voice section values-->
<tr>
	<td>include</td>
	<td>partials/change_voice_section_values</td>
	<td>voice_section_prefix=${tab_1}_${index_1}__voice_sections__${index_2},rest=${rest},octave_adjustment=${octave_adjustment},speed_scale_factor=${speed_scale_factor},self_similarity_iterations=${self_similarity_iterations},pitch=${pitch},rhythm=${rhythm},volume=${volume},apply_inversion=${apply_inversion},apply_retrograde=${apply_retrograde}</td>
</tr>
<!--Switch to the other tab-->
<tr>
	<td>include</td>
	<td>partials/ajax_action</td>
	<td>command=click,target=${tab_1}_${tab_2}_tab_link</td>
</tr>
<!--Open our voice section-->
<tr>
	<td>include</td>
	<td>partials/ajax_action</td>
	<td>command=click,target=${tab_2}_${index_2}_voice_sections_show_link,value=</td>
</tr>
<tr>
	<td>include</td>
	<td>partials/expand_fieldset</td>
	<td>toggle_link_id=${tab_2}_${index_2}__voice_sections__${index_1}_show_link</td>
</tr>
<!--Check that our values have been persisted-->
<tr>
	<td>include</td>
	<td>partials/verify_voice_section_values</td>
	<td>voice_section_prefix=${tab_2}_${index_2}__voice_sections__${index_1},rest=${rest},octave_adjustment=${octave_adjustment},speed_scale_factor=${speed_scale_factor},self_similarity_iterations=${self_similarity_iterations},pitch=${pitch},rhythm=${rhythm},volume=${volume},apply_inversion=${apply_inversion},apply_retrograde=${apply_retrograde}</td>
</tr>
<tr>
	<td>include</td>
	<td>partials/collapse_fieldset</td>
	<td>fieldset_id=${tab_2}_${index_2}__voice_sections__${index_1}_fieldset</td>
</tr>

